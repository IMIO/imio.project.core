<tal:defines define="batch python: view.listRenderedContainedElements(widgets_to_render=('dummy_unexisting_field_name'),
                                                                      b_size=30,
                                                                      b_start=request.get('b_start', 0))">
<div id="imio-folder-listing">
<table id="imio-folder-listing-table"
       class="listing"
       tal:condition="batch">
    <tr>
        <th class='imio_actions_header_title' i18n:translate="" i18n:domain="imio.project.core">Title</th>
        <th class='imio_actions_header_description' i18n:translate="" i18n:domain="imio.project.core">Description</th>
        <th class='imio_actions_header_state' i18n:translate="" i18n:domain="imio.project.core">State</th>
        <th class='imio_actions_header_creator' i18n:translate="" i18n:domain="imio.project.core">Creator</th>
    </tr>
    <tr tal:repeat="renderedElement batch">
        <tal:defines define="obj python: renderedElement.context;
                             widgets renderedElement/widgets;">
        <td><a href="#" class="#"
               tal:content="obj/title"
               tal:attributes="href obj/absolute_url;
                               class string:contenttype-${obj/portal_type};">Object title</a></td>
        <td tal:content="obj/Description">Object description</td>
        <td tal:content="obj/@@plone_context_state/workflow_state" i18n:translate="" i18n:domain="plone">State</td>
        <td tal:content="python: view.author(obj.Creator())">Creator</td>
        </tal:defines>
    </tr>
</table>
<p class="discreet"
   tal:condition="not: batch"
   i18n:domain="plone"
   i18n:translate="description_no_items_in_folder">
    There are currently no items in this folder.
</p>
</div>
</tal:defines>